{
  "name": "Uek-stock-traders",
  "version": "0.1.0",
  "description": "",
  "author": "",
  "homepage": "",
  "scripts": {
    "dev": "bnr dev",
    "build": "bnr build",
    "build-dev": "bnr build-dev",
    "test": "bnr test",
    "test:watch": "bnr test-watch",
    "ionic:serve": "ionic-app-scripts serve",
    "ionic:build": "ionic-app-scripts build",
    "android:dev": "bnr android:dev",
    "android:release": "bnr android:release",
    "release": "standard-version --no-verify",
    "lint": "tslint \"src/**/*.ts\" --project ./tsconfig.json",
    "scss-lint": "scss-lint",
    "prepush": "npm test"
  },
  "betterScripts": {
    "dev": {
      "command": "ionic-app-scripts serve",
      "env": {
        "NODE_ENV": "development",
        "PLATFORM": "web"
      }
    },
    "build": {
      "command": "ionic-app-scripts build --prod --tsconfig ./tsconfig.build.json",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build-dev": {
      "command": "ionic-app-scripts build --dev",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "test": {
      "command": "karma start",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test-watch": {
      "command": "karma start --auto-watch --no-single-run",
      "env": {
        "NODE_ENV": "test",
        "NO_COVERAGE": true
      }
    },
    "android:dev": {
      "command": "ionic cordova run android",
      "env": {
        "NODE_ENV": "development",
        "PLATFORM": "android"
      }
    },
    "android:release": {
      "command": "ionic cordova build android --prod --release",
      "env": {
        "NODE_ENV": "production",
        "PLATFORM": "android"
      }
    }
  },
